<?xml version="1.0" encoding="UTF-8"?>
<desiredgraphs>
  <graph title="Multivariable test" xlabel="Seconds" ylabel="x" >
    <metric label="Pitch" x_stream="pitch"/>
    <metric label="Roll" x_stream="roll" func="x"/>
    <metric label="P + R" x_stream="roll" y_stream="pitch" func="x + (2*y)"/>
    <metric label="Yaw" x_stream="yaw"/>
    <metric label="Yaw2" x_stream="yaw" func="x"/>
    <metric label="Yaw" x_stream="yaw"/>
  </graph>
  <graph title="Text output" xlabel="Seconds" ylabel="x" output="text" >
    <metric label="Yaw" x_stream="yaw"/>
    <metric label="Yaw2" x_stream="yaw" func="x"/>
    <metric label="Yaw3" x_stream="yaw"/>
  </graph>
</desiredgraphs>