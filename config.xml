<?xml version="1.0" encoding="UTF-8"?>
<desiredgraphs>
  <graph title="Multivariable test" xlabel="Seconds" ylabel="x" >
    <metric label="Pitch" x_stream="pitch" func="x" color="yellow"></metric>
    <metric label="Roll" x_stream="roll" func="x" color="red"></metric>

    <metric label="P + R" x_stream="roll" y_stream="pitch" func="x + (2*y)" color="blue"></metric>
  </graph>
  <graph title="Test 1" xlabel="Seconds" ylabel="Pitch" legend="no">
    <metric label="Main" x_stream="pitch" func="x"></metric>
    <metric label="Target" x_stream="target_pitch_velocity" func="x"></metric>
  </graph>
  <graph title="Test 2" xlabel="Seconds" ylabel="Roll">
    <metric label="Main" x_stream="pitch" func="x" color="blue"></metric>
    <metric label="Target" x_stream="target_roll_velocity" func="x"></metric>
    <metric label="Testeroni" x_stream="pitch" func="x/3"></metric>
  </graph>
</desiredgraphs>